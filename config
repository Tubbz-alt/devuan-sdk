#!/usr/bin/env zsh

# Devuan SDK configuration
# syntax: variable=value
# (no spaces before and after the equal sign)

mirror=http://ftp.debian.org/debian/
release=jessie
section=main


# deboostrap settings for chroots
chroot_include=bzip2,zsh,vim-nox,sysvinit,debian-keyring
chroot_exclude=systemd,systemd-sysv,libsystemd0
# safe to leave untouched
chroot_variant=buildd
chroot_comp=main # components
